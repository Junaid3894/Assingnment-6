/*Write a program to calculate sum of first N odd natural numbers*/
#include<stdio.h>
int main()
{
int i;
int sum=0;
int n;
// input upper limit from user
printf("Enter the n :");
scanf("%d",&n);
for(i=1;i<=n;i++){
    // check odd codition before assining a value 
    if(i%2){
    sum+=i;
    }
}
printf("%d",sum);
return 0;
}
