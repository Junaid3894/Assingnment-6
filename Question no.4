/*Write a program to calculate sum of squares of first N natural numbers*/
#include<stdio.h>
int main()
{
int i,square;
int sum=0;
int n;
// input upper limit from user
printf("Enter the n :");
scanf("%d",&n);
for(i=1;i<=n;i++){
    square=i*i;
    // sum variable add square value in each iteration
    sum+=square;
}
printf("%d",sum);
return 0;
}
