/*Write a program to calculate sum of cubes of first N natural numbers*/
#include<stdio.h>
int main()
{
int i,cubes;
int sum=0;
int n;
// input upper limit from user
printf("Enter the n :");
scanf("%d",&n);
for(i=1;i<=n;i++){
    cubes=i*i*i;
    // sum variable add cubes value in each iteration
    sum+=cubes;
}
// finally sum will print
printf("%d",sum);
return 0;
}
